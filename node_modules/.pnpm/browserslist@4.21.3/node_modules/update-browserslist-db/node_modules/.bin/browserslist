#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/HSDN/Desktop/smarty-ui-vite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/HSDN/Desktop/smarty-ui-vite/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.21.3/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@4.21.3/node_modules/browserslist/cli.js" "$@"
fi
